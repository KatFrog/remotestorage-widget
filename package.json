{
  "name": "remotestorage-widget",
  "version": "1.0.0-rc5",
  "description": "remoteStorage.js connect widget",
  "main": "build/widget.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf build/*",
    "prebuild": "npm run clean -s",
    "build": "NODE_ENV=production webpack -p",
    "dev": "webpack-dev-server --inline --hot --content-base demo --port 8008",
    "open": "opener http://localhost:8008",
    "version": "npm run build && git add build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remotestorage/remotestorage-widget.git"
  },
  "keywords": [
    "remotestorage",
    "remotestoragejs",
    "unhosted",
    "no-backend",
    "offline-first"
  ],
  "author": "RS Contributors",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remotestorage/remotestorage-widget/issues"
  },
  "homepage": "https://github.com/remotestorage/remotestorage-widget#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "http-server": "^0.8.5",
    "inline-loader": "^0.1.1",
    "inline-svg-loader": "0.0.1",
    "opener": "^1.4.1",
    "raw-loader": "^0.5.1",
    "remotestoragejs": "1.0.0-rc5",
    "rimraf": "^2.4.3",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
